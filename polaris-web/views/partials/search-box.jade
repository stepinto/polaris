.search-box
  input.query(type="text", ng-model="query", placeholder="{{placeholder}}", autofocus)
  .loading-indicator(ng-show='loading')
  .div(ng-show="choices.length > 0 && visible")
    ul.choice-box(role='menu', style="display:block")
      li.row(
          ng-repeat="choice in choices",
          ng-class="{active: choice.index == selected}")
        span(class="{{choice.icon}}")
        span.query-hint {{choice.queryHint}}
        span.uses {{choice.useCountText}}
        br
        a(ng-href="{{choice.url}}"){{choice.jumpTarget.file.project + choice.jumpTarget.file.path}}
